{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/startServer.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/startServer.ts",
    "build": "echo 'Skipping build due to TS errors'",
    "prestart": "echo 'Skipping build step due to Render type error'",
    "start": "ts-node --transpile-only src/startServer.ts",
    "docker:build": "docker build --tag snap-tweet-api --file ./Dockerfile ../..",
    "docker:run": "docker run --rm --publish 5000:5000 snap-tweet-api",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --coverage --runTestsByPath tests/integration/",
    "coverage-badge": "jest-coverage-badges"
  },
  "dependencies": {
    "backend": "file:",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^6.8.3",
    "multer": "^1.4.5-lts.2",
    "multer-storage-cloudinary": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}